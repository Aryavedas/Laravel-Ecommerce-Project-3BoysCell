{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
        {
            "src": "/(css|js|images|storage|build)/.*",
            "dest": "/public/$0"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://3boyscell.vercel.app",
        "ASSET_URL": "https://3boyscell.vercel.app"
    }
}